  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#5C6370" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#98C379" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#D19A66" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#C678DD" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#61AFEF" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#ABB2BF" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#E5C07B" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#D19A66" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#282C34", "background": "#E06C75" } }
    ]
  }
}

