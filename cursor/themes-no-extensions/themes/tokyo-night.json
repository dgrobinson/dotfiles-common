  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#565F89" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#E0AF68" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#BB9AF7" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#F7768E" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#9ECE6A" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#A9B1D6" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#7AA2F7" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#FF9E64" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#1A1B26", "background": "#F7768E" } }
    ]
  }
}

