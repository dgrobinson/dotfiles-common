  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#586E75" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#2AA198" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#B58900" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#859900" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#268BD2" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#93A1A1" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#6C71C4" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#CB4B16" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#002B36", "background": "#DC322F" } }
    ]
  }
}

