  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#6272A4" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#F1FA8C" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#BD93F9" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#FF79C6" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#50FA7B" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#F8F8F2" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#8BE9FD" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#FFB86C" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#FFFFFF", "background": "#FF5555" } }
    ]
  }
}

