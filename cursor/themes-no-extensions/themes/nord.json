  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#4C566A" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#A3BE8C" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#B48EAD" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#81A1C1" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#88C0D0" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#D8DEE9" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#8FBCBB" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#EBCB8B" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#2E3440", "background": "#BF616A" } }
    ]
  }
}

