  "editor.tokenColorCustomizations": {
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#928374" } },
      { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#B8BB26" } },
      { "scope": ["constant", "variable.other.constant"], "settings": { "foreground": "#D3869B" } },
      { "scope": ["keyword", "storage"], "settings": { "foreground": "#FB4934" } },
      { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#83A598" } },
      { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#EBDBB2" } },
      { "scope": ["entity.name.type", "support.class", "storage.type"], "settings": { "foreground": "#8EC07C" } },
      { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#FABD2F" } },
      { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#282828", "background": "#FB4934" } }
    ]
  }
}

